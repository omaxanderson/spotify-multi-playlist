{
   "name": "spotify-multi-track",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "server": "npx nodemon --exec ts-node index.ts",
      "client": "npx webpack-dev-server --host 0.0.0.0 --disable-host-check",
      "start:dev": "concurrently \"npm run server\" \"npm run client\"",
      "start": "npm run build:client && npm run build:server && node build/index.js",
      "serve:prod": "concurrently \"npm run build:client\" \"npm run build:server\" && npx babel-node build/index.js",
      "build:client": "npx webpack",
      "build:server": "tsc",
      "server:prod": "node build/index.js",
      "docker:build-client": "docker build -f Dockerfile-client --tag omaxanderson/client .",
      "docker:build-server": "docker build -f Dockerfile --tag omaxanderson/server .",
      "test": "echo \"Error: no test specified\" && exit 1"
   },
